import java.util.*;
class Main{
    public static void main(String[] args) {
        Main obj=new Main();
        obj.add(2,3);
        System.out.println("Hello!");
    }
    void add(int a,int b){
        int x=a;
        int y=b;
        int z=x+y;

        System.out.println("sum of "+z);
    }
}
